varnishtest "test vmod-dynmetrics"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
        import dynmetrics;

        sub vcl_deliver {
	    set resp.http.Hello = dynmetrics.hello();
        }
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
	expect resp.http.Hello == "vmod-dynmetrics"
} -run
